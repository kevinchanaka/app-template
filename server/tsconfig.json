{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
  },
    "ts-node": {
      "compilerOptions": {
        "module": "esnext"
      },
      "transpileOnly": true,
      "esm": true
    }
}
